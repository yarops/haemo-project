{
  "name": "yarops/haemo",
  "description": "Theme haemo",
  "type": "wordpress-theme",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Yaroslav Popov",
      "email": "info@codesweet.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
