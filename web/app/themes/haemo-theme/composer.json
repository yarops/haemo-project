      "type": "git"
    }
  ],
  "require": {
    "php": "^8.2",
    "chillerlan/php-qrcode": "dev-main"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
